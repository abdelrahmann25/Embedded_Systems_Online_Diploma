//Program to compare entered string with another entered string list.

#include <stdio.h>

int main(void) {
	while(1)
	{
	char str1[50][50];
	char str2[50][50];
	int r;
	for(r=0;str1[r][0]!='\0';r++)
	{
	printf("Enter string%d : ",r+1);
	fflush(stdin); fflush(stdout);
	gets(str1[r]);
	printf("If you want to end the list type 0");
	}

	for(r=0;str1[r]!='\0';r++)
	{
	printf("%s\n",str1[r]);
	}
	}
	return 0;
}
